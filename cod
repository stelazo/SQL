SELECT
    FATO.COMP_GUIA,
    FATO.DT_ATEND,
    FATO.COD_GUIA_PRINC,
    FATO.VLR_TOTAL_PAGO,
    PRESTADOR.PRES_EXE_NOME_PRESTADOR,
    PRESTADOR.PRES_EXEC_ID,
    ITEM.ITEM_CODIGO,
    ITEM.ITEM_DESCRICAO,
    BENEFICIARIO.BENF_NOME,
    BENEFICIARIO.BENF_ID 
FROM
    DWSGU.FATO_PRODUCAO FATO
JOIN
    DWSGU.PRESTADOR_EXECUTANTE PRESTADOR ON FATO.PRES_EXEC_ID = PRESTADOR.PRES_EXEC_ID
JOIN
    DWSGU.ITEM ITEM ON FATO.ITEM_ID = ITEM.ITEM_ID 
JOIN
    DWSGU.BENEFICIARIO BENEFICIARIO ON FATO.BENF_ID = BENEFICIARIO.BENF_ID 
WHERE
	/*(PRESTADOR.PRES_EXEC_ID IN (640008018657, 640008010672) OR 
    (PRESTADOR.PRES_EXE_NOME_FANTASIA IS NULL OR PRESTADOR.PRES_EXE_NOME_FANTASIA = '')) AND*/ 
    (FATO.COD_GUIA_PRINC IN ()) AND
    ((ITEM.ITEM_CODIGO IN ()) ;
